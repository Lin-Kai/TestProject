<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="生成密码" Height="268.568" Width="492.706" FontFamily="Consolas,Microsoft YaHei UI"  FontSize="12
        ">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="293*"/>
            <ColumnDefinition Width="192*"/>
        </Grid.ColumnDefinitions>
        <TextBox Name="inputTextBox" HorizontalAlignment="Left" Height="46" Margin="10,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" AcceptsReturn="True" FontSize="18"/>
        <Button Content="生成！" HorizontalAlignment="Left" Margin="292,10,0,0" VerticalAlignment="Top" Width="116" Height="46" Click="Button_Click" FontSize="24" FontFamily="Segoe UI Semibold" Grid.ColumnSpan="2" />
        <TextBox Name="outputTextBlock" Margin="10,104,10,10" TextWrapping="Wrap" Text="" FontFamily="consolas" FontSize="18" Grid.ColumnSpan="2"/>
        <Slider Name="slider" HorizontalAlignment="Left" Margin="10,72,0,0" VerticalAlignment="Top" Width="120" SmallChange="0.001"/>
        <CheckBox Name="chkSymbol" Content="符号" HorizontalAlignment="Left" Margin="149,10,0,0" VerticalAlignment="Top"/>
        <RadioButton Name="capital_mix" Content="大小写混合" GroupName="capital" HorizontalAlignment="Left" Margin="149,38,0,0" VerticalAlignment="Top" IsChecked="True"/>
        <RadioButton Name="capital_cap" Content="只有大写" GroupName="capital" HorizontalAlignment="Left" Margin="149,57,0,0" VerticalAlignment="Top"/>
        <RadioButton Name="capital_low" Content="只有小写" GroupName="capital" HorizontalAlignment="Left" Margin="149,76,0,0" VerticalAlignment="Top"/>
        <!--
        <RadioButton Name="num_mix" Content="英文数字混合" HorizontalAlignment="Left" GroupName="num" Margin="228,38,0,0" VerticalAlignment="Top"/>
        <RadioButton Name="num_eng" Content="纯英文" HorizontalAlignment="Left" GroupName="num" Margin="228,57,0,0" VerticalAlignment="Top"/>
        <RadioButton Name="num_num" Content="纯数字" HorizontalAlignment="Left" GroupName="num" Margin="228,76,0,0" VerticalAlignment="Top"/>
-->
    </Grid>
</Window>
